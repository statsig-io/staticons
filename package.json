{
  "name": "staticons",
  "version": "1.0.0",
  "description": "Monorepo for packaging Statsig Product Icons a.k.a Staticons for various frameworks",
  "main": "dist/index.js",
  "scripts": {
    "build:lib": "pnpm --dir internal/lib build",
    "build:scripts": "pnpm --dir internal/scripts build",
    "build:react": "pnpm --dir packages/staticons-react build",
    "build:site": "pnpm --dir site build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/statsig-io/staticons.git"
  },
  "author": "kenny@statsig.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/statsig-io/staticons/issues"
  },
  "homepage": "https://github.com/statsig-io/staticons#readme",
  "workspaces": [
    "packages/*",
    "internal/*",
    "site/*"
  ],
  "devDependencies": {
    "@11ty/eleventy": "^3.0.0",
    "@types/node": "^22.14.0",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.2",
    "@types/typescript": "^2.0.0",
    "vite": "^6.2.6"
  },
  "dependencies": {
    "@vitejs/plugin-react": "^4.3.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "private": true
}